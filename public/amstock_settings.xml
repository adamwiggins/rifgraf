  <!-- DATE FORMATS define how dates should be formatted. There are two places where dates are 
  displayed: on x axis and in the legend. Different time intervals require different date formatting, 
  that's why you need to set a separate date format for every interval. "month" will be replaced with
  name of the month. Names of the months can be defined in <strings> section -->
  <date_formats>        
     <x_axis>     
       <!-- [hh:mm:ss] -->
       <seconds></seconds>
       
       <!-- [hh:mm] -->
       <minutes></minutes>
       
       <!-- [hh:mm] -->   
       <hours></hours>
       
       <!-- [month DD] -->
       <days></days>
       
       <!-- [month] -->
       <months></months>
       
       <!-- [YYYY] -->
       <years></years>
     </x_axis>
     <legend>
       <!-- [hh:mm:ss] -->
       <seconds></seconds>
       
       <!-- [hh:mm] -->  
       <minutes></minutes>
       
       <!-- [hh:mm] -->   
       <hours></hours>
       
       <!-- [month DD, YYYY] -->
       <days></days>
       
       <!-- [week of month DD, YYYY] -->
       <weeks></weeks>
       
       <!-- [month YYYY] -->
       <months></months>
       
       <!-- [YYYY] -->       
       <years></years>
     </legend>
  </date_formats>  

